{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "**/*.ts",
        "**/*.tsx",
        "!node_modules/**"
    ],
    "files": [
        "app/app.int.ts",
        "app/app.red.ts",
        "app/app.spec.ts",
        "app/components/sidebar/sidebar.act.ts",
        "app/components/sidebar/sidebar.red.ts",
        "app/pages/doc/doc.act.ts",
        "app/pages/doc/doc.red.ts",
        "app/services/array/includes.ts",
        "app/services/array/index.ts",
        "app/services/array/key.ts",
        "app/services/array/move.ts",
        "app/services/array/remove.ts",
        "app/services/class/add-class.ts",
        "app/services/class/get-parent.ts",
        "app/services/class/has-class.ts",
        "app/services/class/index.ts",
        "app/services/class/remove-class.ts",
        "app/services/immutable/immutable.ts",
        "app/services/md5/md5.ts",
        "app/services/pretty-date/pretty-date.ts",
        "app/services/string/camelcase-to-dash.ts",
        "app/services/string/decodeEntities.ts",
        "typings/jasmine/jasmine.d.ts",
        "typings/node/node.d.ts",
        "typings/react-redux/react-redux.d.ts",
        "typings/react-router/history.d.ts",
        "typings/react-router/react-router.d.ts",
        "typings/react/react-addons-test-utils.d.ts",
        "typings/react/react-dom.d.ts",
        "typings/react/react.d.ts",
        "typings/redux-thunk/redux-thunk.d.ts",
        "typings/redux/redux.d.ts",
        "typings/tsd.d.ts",
        "app/app.tsx",
        "app/components/content/content.tsx",
        "app/components/hamburger/hamburger.tsx",
        "app/components/logo/logo.tsx",
        "app/components/search-form/search-form.tsx",
        "app/components/sidebar/sidebar.tsx",
        "app/pages/doc/doc.tsx",
        "app/reusable/input-switch/input-switch.tsx"
    ],
    "exclude": [],
    "atom": {
        "rewriteTsconfig": true
    },
    "compileOnSave": false
}
